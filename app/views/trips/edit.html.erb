<h1><%= @trip.title %></h1>

<div id="photos"></div>

<%= form_for Photo.new, :url => trip_upload_photos_path(current_user, @trip), :html => {:multipart => true}, id: 'new_photo' do |f| %>
    <div class="field">
      <%= f.file_field :image, multiple: true, name: 'photo[image]' %>
    </div>
<% end %>


<div id='gmaps-error'></div>

<div id='gmaps-canvas'></div>

<%= link_to 'Done', dashboard_path %>


<script id="template-upload" type="text/x-tmpl">
<div class="upload">
  {%=o.name%}
  <div class="progress"><div class="bar" style="width: 0%"></div></div>
</div>
</script>